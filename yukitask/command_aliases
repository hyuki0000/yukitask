# vim: set filetype=sh:
if test "$EDITOR" = "" ; then
  echo "YukiTask: Warning: 'EDITOR' is not set."
fi
alias mk='touch_task; make'
alias bk='make bk'
alias e='make e'
alias show='make show'
alias main='make main'
alias m='$EDITOR makefile'
alias T='$EDITOR TODO'
alias TT='$EDITOR ~/yukitask/TODO'
